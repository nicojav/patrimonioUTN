<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UTN\Bundle\InventariosBundle\Entity\Aula" table="aula">
    <indexes>
      <index name="IDX_31990A4A1BBFED3" columns="id_sede"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UK_aula" columns="cod_aula"/>
    </unique-constraints>
    <id name="idAula" type="integer" column="id_aula">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codAula" type="string" column="cod_aula" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="descripcion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idSede" target-entity="Sede" fetch="LAZY">
      <join-columns>
        <join-column name="id_sede" referenced-column-name="id_sede"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
